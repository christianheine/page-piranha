    "gemini-flash",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianheine/page-piranha.git"
  },
  "homepage": "https://github.com/christianheine/page-piranha",
  "bugs": {
    "url": "https://github.com/christianheine/page-piranha/issues"
  },
  "author": "Christian Heine",
  "license": "MIT",
  "dependencies": {
    "chalk": "5.4.1",
    "commander": "13.1.0",
    "dotenv": "16.4.7",
    "jorel": "0.11.2",
    "ora": "8.2.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "prettier": "3.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1"
  },
  "engines": {
    "node": ">=18"
  }
