{
  "name": "page-piranha",
  "version": "0.1.3",
  "description": "LLM based page parser (PDF to text)",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "rebuild": "npm run clean && npm run build"
  },
  "bin": {
    "page-piranha": ".bin/page-piranha.js"
  },
  "keywords": [
    "pdf",
    "convert",
    "pdf-to-text",
